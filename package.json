{
  "name": "webpack-react",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Michael Pontus <m.pontus@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "start:dev": "BUILD_ENV=development webpack-dev-server --mode development",
    "build": "NODE_OPTIONS=--openssl-legacy-provider webpack --mode production",
    "build:dev": "BUILD_ENV=development NODE_OPTIONS=--openssl-legacy-provider webpack --mode production",
    "build:stage": "BUILD_ENV=staging NODE_OPTIONS=--openssl-legacy-provider webpack --mode production",
    "build:prod": "BUILD_ENV=production NODE_OPTIONS=--openssl-legacy-provider webpack --mode production",
    "build:all": "yarn build:dev && yarn build:stage && yarn build:prod",
    "build:analyze": "ANALYZE=true BUILD_ENV=production NODE_OPTIONS=--openssl-legacy-provider webpack --mode production",
    "build:stats": "NODE_OPTIONS=--openssl-legacy-provider webpack --mode production --profile --json > stats.json",
    "serve": "firebase serve --only hosting",
    "serve:dev": "npx serve dist-dev -s -p 3000 --no-compression",
    "serve:stage": "npx serve dist-stage -s -p 3001",
    "serve:prod": "npx serve dist-prod -s -p 3002",
    "deploy": "firebase deploy --only hosting",
    "deploy:dev": "yarn build:dev && firebase deploy --only hosting:dev",
    "deploy:stage": "yarn build:stage && firebase deploy --only hosting:stage",
    "deploy:prod": "yarn build:prod && firebase deploy --only hosting:prod",
    "deploy:all": "yarn build:all && firebase deploy --only hosting",
    "preview": "firebase hosting:channel:deploy preview",
    "clean": "rm -rf dist-dev dist-stage dist-prod",
    "rebuild": "yarn clean && yarn build:all",
    "test:local": "yarn build:dev && npx serve dist-dev -s -p 5002 --no-compression",
    "analyze": "yarn build:analyze",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPatterns='__tests__'",
    "test:integration": "jest --testPathPatterns='__integration__'",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --reporters=default --reporters=jest-junit",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "prepare": "husky"
  },
  "dependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "6"
  },
  "resolutions": {
    "get-intrinsic": "1.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-react-constant-elements": "^7.6.0",
    "@babel/plugin-transform-react-inline-elements": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.6.0",
    "@eslint/js": "^9.39.2",
    "@svgr/webpack": "^4.3.2",
    "@testing-library/dom": "8",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "12",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "ansi-html-community": "^0.0.8",
    "babel-jest": "^30.2.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "compression-webpack-plugin": "^6.1.1",
    "css-loader": "^3.2.0",
    "eslint": "^9.39.2",
    "eslint-plugin-jest": "^29.12.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "file-loader": "^4.2.0",
    "globals": "^17.0.0",
    "html-entities": "^2.6.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^16.2.7",
    "loglevel": "^1.9.2",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "qs": "^6.14.1",
    "react-test-renderer": "16",
    "sockjs-client": "^1.6.1",
    "strip-ansi": "^7.1.2",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^2.1.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.40.2",
    "webpack-bundle-analyzer": "^5.1.1",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "jest --bail --findRelatedTests --passWithNoTests"
    ],
    "src/**/*.{css,scss}": [
      "echo 'CSS files staged'"
    ]
  }
}
